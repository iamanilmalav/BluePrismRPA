" /><inputs><input type="text" name="File Name" friendlyname="File Name" narrative="The name of the file to check" expr="[PDF2]" /></inputs><outputs><output type="flag" name="Exists" friendlyname="Exists" narrative="True if the file exists" stage="Exists" /></outputs><onsuccess>b9666a07-1954-4041-a55b-cc4ca5c93d01</onsuccess><resource object="Utility - File Management" action="File Exists" /></stage><stage stageid="b9666a07-1954-4041-a55b-cc4ca5c93d01" name="Check File Path Exists?" type="Decision"><subsheetid>c7b39dc7-b399-4a97-96fc-75c3380aaadf</subsheetid><loginhibit /><display x="30" y="135" /><decision expression="[Exists]" /><ontrue>f97145c8-30d9-489b-b39f-58d236ab1890</ontrue><onfalse>4a2a9ec9-3037-4830-9ba8-c97cf5c82809</onfalse></stage><stage stageid="4a2a9ec9-3037-4830-9ba8-c97cf5c82809" name="File Not Found" type="Exception"><subsheetid>c7b39dc7-b399-4a97-96fc-75c3380aaadf</subsheetid><loginhibit /><display x="135" y="135" /><exception localized="yes" type="File Not Found" detail="&quot;PDF file not present in the provided File Path&quot;" /></stage><stage stageid="f97145c8-30d9-489b-b39f-58d236ab1890" name="Merge PDF" type="Code"><subsheetid>c7b39dc7-b399-4a97-96fc-75c3380aaadf</subsheetid><loginhibit /><display x="30" y="195" /><inputs><input type="text" name="pdf_1" expr="[PDF1]" /><input type="text" name="pdf_2" expr="[PDF2]" /><input type="text" name="pdf_out" expr="[OutputPDF]" /></inputs><outputs><output type="flag" name="result" stage="Success" /></outputs><onsuccess>284662a3-df1c-40a2-b3c6-48b6e1f2c56e</onsuccess><code><![CDATA[Dim pdfCount As Integer = 0
Dim f As Integer = 0
Dim fileName As String
Dim reader As iTextSharp.text.pdf.PdfReader = Nothing
Dim pageCount As Integer = 0
Dim pdfDoc As iTextSharp.text.Document = Nothing
Dim writer As PdfWriter = Nothing
Dim cb As PdfContentByte = Nothing

Dim page As PdfImportedPage = Nothing
Dim rotation As Integer = 0

Try

'Open the first PDF File
	fileName = pdf_1
	reader = New iTextSharp.text.pdf.PdfReader(fileName)

'Get page count
	pageCount = reader.NumberOfPages
	pdfDoc = New iTextSharp.text.Document(reader.GetPageSizeWithRotation(1), 18, 18, 18, 18)
	writer = PdfWriter.GetInstance(pdfDoc, New FileStream(pdf_out, FileMode.OpenOrCreate))

With pdfDoc
	.Open()
End With

'Instantiate a PdfContentByte Object
	cb = writer.DirectContent

'Loop though the input PDFs
Dim i As Integer = 0

While i < pageCount

	i += 1

'Get the input page size
	pdfDoc.SetPageSize(reader.GetPageSizeWithRotation(i))

'Create a new page on the output document
	pdfDoc.NewPage()

'Add bookmark to the first page and get the imported page

	page = writer.GetImportedPage(reader, i)

'Read the imported page rotation
	rotation = reader.GetPageRotation(i)

'Add the imported page to the PdfContentByte object as a template based on the page's rotation
	If rotation = 90 Then

		cb.AddTemplate(page, 0, -1.0F, 1.0F, 0, 0, reader.GetPageSizeWithRotation(i).Height)

	ElseIf rotation = 270 Then

		cb.AddTemplate(page, 0, 1.0F, -1.0F, 0, reader.GetPageSizeWithRotation(i).Width + 60, -30)

	Else cb.AddTemplate(page, 1.0F, 0, 0, 1.0F, 0, 0)

	End If

End While 

'Read second PDF File
	fileName = pdf_2
	reader = New iTextSharp.text.pdf.PdfReader(fileName)
	pageCount = reader.NumberOfPages 

'Declare a page counter variable
	i = 0

'Loop through the current input pdf's pages starting at page 1
While i < pageCount
	i += 1

'Get the input page size
	pdfDoc.SetPageSize(reader.GetPageSizeWithRotation(i))

'If first page, add bookmarks to the page and get the imported page
	page = writer.GetImportedPage(reader, i)

'Read the imported page's rotation
	rotation = reader.GetPageRotation(i)

'Then add the imported page to the PdfContentByte object as a template based on the page's rotation
	If rotation = 90 Then

		cb.AddTemplate(page, 0, -1.0F, 1.0F, 0, 0, reader.GetPageSizeWithRotation(i).Height)

	ElseIf rotation = 270 Then

		cb.AddTemplate(page, 0, 1.0F, -1.0F, 0, reader.GetPageSizeWithRotation(i).Width + 60, -30)

	Else

		cb.AddTemplate(page, 1.0F, 0, 0, 1.0F, 0, 0)

	End If

End While 

'When all done, we close the document so that the pdfwriter object can write it to the output file
	pdfDoc.Close()
	result = True

Catch ex As Exception
	result = False

End Try]]></code></stage><stage stageid="6d5cc60a-53d8-4020-acba-2e7abfa2a282" name="Success" type="Data"><subsheetid>c7b39dc7-b399-4a97-96fc-75c3380aaadf</subsheetid><loginhibit /><display x="-195" y="240" w="120" h="30" /><datatype>flag</datatype><initialvalue>False</initialvalue><private /><alwaysinit /></stage><stage stageid="4b08730a-aaa1-4c67-b242-ec230f4111ac" name="Remove Empty Rows" type="Action"><subsheetid>22db69fa-a1ad-4b1f-acdb-977e60d1f138</subsheetid><loginhibit /><display x="75" y="150" /><inputs><input type="collection" name="Collection In" friendlyname="Collection In" narrative="The collection to remove empty rows from" expr="[Split Values]" /></inputs><outputs><output type="collection" name="Collection Out" friendlyname="Collection Out" narrative="The collection with the empty rows removed" stage="Split Values" /></outputs><onsuccess>80b23949-6a1c-427c-9ce3-82581ef53f14</onsuccess><resource object="Utility - Collection Manipulation" action="Remove Empty Rows" /></stage></process></object>
