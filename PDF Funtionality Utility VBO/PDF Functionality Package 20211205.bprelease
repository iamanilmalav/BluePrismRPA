" /><input type="text" name="Collection Field Name" friendlyname="Collection Field Name" narrative="The name of the field for the resulting collection" expr="&quot;PageIndex&quot;" /></inputs><outputs><output type="collection" name="Split Values" friendlyname="Split Values" narrative="The resulting collection containing the split values" stage="Split Values" /></outputs><onsuccess>4b08730a-aaa1-4c67-b242-ec230f4111ac</onsuccess><resource object="Utility - Strings" action="Split Text" /></stage><stage stageid="8ca8ecd1-4e64-4e8c-a1d0-b63e64ea4f3a" name="Split Values" type="Collection"><subsheetid>22db69fa-a1ad-4b1f-acdb-977e60d1f138</subsheetid><loginhibit /><display x="-225" y="135" w="120" h="30" /><datatype>collection</datatype><private /><alwaysinit /></stage><stage stageid="c1463035-cf69-4c8d-b21c-34d14246353d" name="PageTextCol" type="Collection"><subsheetid>22db69fa-a1ad-4b1f-acdb-977e60d1f138</subsheetid><loginhibit /><display x="-225" y="225" w="120" h="30" /><datatype>collection</datatype><private /><alwaysinit /><collectioninfo><field name="PageIndex" type="number" description="Page Index for which text is extracted" /><field name="PageText" type="text" description="Page Text" /></collectioninfo></stage><stage stageid="80b23949-6a1c-427c-9ce3-82581ef53f14" name="Get Selected Page Text" type="Code"><subsheetid>22db69fa-a1ad-4b1f-acdb-977e60d1f138</subsheetid><loginhibit /><display x="75" y="195" /><inputs><input type="text" name="FilePath" expr="[FilePath]" /><input type="collection" name="PageIndexes" expr="[Split Values]" /></inputs><outputs><output type="collection" name="PageTextCol" stage="PageTextCol" /></outputs><onsuccess>fb934186-b150-4774-94a0-c0bfeb6a3802</onsuccess><code><![CDATA['Load the PDF File Path
Dim pr As New PdfReader(FilePath)

'Get Number of Pages'
Dim pageCount As Integer = 0
	pageCount = pr.NumberOfPages

'Initialize Data Table
Dim s As String = ""
Dim dt As New DataTable
	dt.Columns.AddRange(New DataColumn(1) {New DataColumn("PageIndex"), New DataColumn("PageText")})

'Sort Page Indexes
	PageIndexes.DefaultView.Sort = "PageIndex ASC"
	PageIndexes = PageIndexes.DefaultView.ToTable


'Extract PDF Text Page Wise
Dim its As ITextExtractionStrategy
	its = New LocationTextExtractionStrategy

For Each row  As DataRow In PageIndexes.Rows
	If (row("PageIndex") > pageCount) Then
	
		dt.Rows.Add(row("PageIndex"), "Error : Page Index Outside Boundary")

	Else
	
		s = PdfTextExtractor.GetTextFromPage(pr, row("PageIndex"), its)
		dt.Rows.Add(row("PageIndex"), s)
	
	End If
Next


'Get Output TextCol
PageTextCol = dt]]></code></stage><stage stageid="f603bedb-f37d-472b-9f60-bebf2289464d" name="Get All Page Text" type="Code"><subsheetid>22db69fa-a1ad-4b1f-acdb-977e60d1f138</subsheetid><loginhibit /><display x="-30" y="195" /><inputs><input type="text" name="FilePath" expr="[FilePath]" /></inputs><outputs><output type="collection" name="PageTextCol" stage="PageTextCol" /></outputs><onsuccess>ae73c4fe-4f28-464b-b45c-704eff943448</onsuccess><code><![CDATA['Load the PDF File Path
Dim pr As New PdfReader(FilePath)

'Get Number of Pages'
Dim pageCount As Integer = 0
	pageCount = pr.NumberOfPages

'Initialize Data Table
Dim s As String = ""
Dim dt As New DataTable
	dt.Columns.AddRange(New DataColumn(1) {New DataColumn("PageIndex"), New DataColumn("PageText")})


'Extract PDF Text Page Wise
Dim its As ITextExtractionStrategy
	its = New LocationTextExtractionStrategy

For i = 1 To pageCount

	s = PdfTextExtractor.GetTextFromPage(pr, i, its)
	dt.Rows.Add(i, s)
	
Next


